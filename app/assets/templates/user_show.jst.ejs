<header class="home"></header>

<main class="home group">
  <sidebar class="libraries">
    <h3><%= user.escape("username") %>'s Libraries:</h3>
    <ul class="library-list"></ul>

  </sidebar>

  <div class="home-content">
    <section class="library-show">
      <div class="lib-show-prompt">
        <p>Click on one of <%= user.escape("username") %>'s libraries to see their movies.</p>
      </div>
    </section>

    <section class="reviews">
      <h3><%= user.escape("username") %>'s Recent Reviews:</h3>
      <ul class="my-review-list"></ul>
    </section>
  </div>
</main>
