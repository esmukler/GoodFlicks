<label>Title:
  <input type="text" name="library[title]" value="<%= library.escape("title") %>">
</label>

<label>Public
  <input type="radio" name="library[is_public]" value="true">
</label>

<label>Private
  <input type="radio" name="library[is_public]" value="false">
</label><br>

<button type="submit">Add Library</button>
