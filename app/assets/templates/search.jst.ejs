<h1>Search</h1>

<label>Enter a Movie Title:
  <input type="text" class="query">
</label>
<button type="submit">Search</button>

<ul class="search-results">
<% results.each (function(result) { %>
  <li><a href="#/movies/<%= result.id %>">
          <%= result.escape("title") %></a></li>
<% }) %>
</ul>
